<?xml version="1.0"?>
<Spase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.spase-group.org/data/schema" 
    xsi:schemaLocation="http://www.spase-group.org/data/schema http://www.spase-group.org/data/schema/spase-2_0_0.xsd">
    <Version>2.0.0</Version>
    <NumericalData>
        <ResourceID>spase://VHO/NumericalData/Pioneer10/MAGandPLS/PT1H</ResourceID>
        <ResourceHeader>
            <ResourceName>Pioneer 10 hourly merged magnetic field and plasma data</ResourceName>
            <ReleaseDate>2008-12-22T00:00:00</ReleaseDate>
            <Description>This is an hourly resolution, merged magnetic field and plasma data set created at NSSDC for COHOWeb.  Magnetic field vectors and plasma flow direction angles are given in RTN coordinates.  Spacecraft position data are given in Heliographic inertial (HGI coordinates).</Description>
            <Acknowledgement>Please acknowledge the magnetic field and plasma Principal Investigators, Drs. E. Smith and P. Gazis, and NSSDC or GSFC-SPDF</Acknowledgement>
            <Contact>
                <PersonID>spase://SMWG/Person/Edward.J.Smith</PersonID>
                <Role>PrincipalInvestigator</Role>
            </Contact>
            <Contact>
                <PersonID>spase://SMWG/Person/Paul.R.Gazis</PersonID>
                <Role>PrincipalInvestigator</Role>
            </Contact>
            <Contact>
                <PersonID>spase://SMWG/Person/Natalia.E.Papitashvili</PersonID>
                <Role>GeneralContact</Role>
            </Contact>
            <InformationURL>
                <Name>nssdcftp readme</Name>
                <URL>ftp://nssdcftp.gsfc.nasa.gov/spacecraft_data/pioneer/pioneer10/merged/p10mgd.txt</URL>
            </InformationURL>
        </ResourceHeader>
        <AccessInformation>
            <RepositoryID>spase://SMWG/Repository/NASA/GSFC/SPDF</RepositoryID>
            <Availability>Online</Availability>
            <AccessRights>Open</AccessRights>
            <AccessURL>
                <Name>nssdcftp</Name>
                <URL>ftp://nssdcftp.gsfc.nasa.gov/spacecraft_data/pioneer/pioneer10/merged/</URL>
                <Description>for annual files via ftp</Description>
            </AccessURL>
            <AccessURL>
                <Name>COHOWeb plots and lists</Name>
                <URL>http://cohoweb.gsfc.nasa.gov/form/pioneer10.html</URL>
                <Description>Pioneer 10-specific page for plots and lists</Description>
            </AccessURL>
            <AccessURL>
                <Name>COHOweb top page</Name>
                <URL>http://cohoweb.gsfc.nasa.gov/</URL>
                <Description>Plots, lists, scatter plots, regressions, distributions, means, etc.</Description>
            </AccessURL>
            <Format>Text</Format>
            <DataExtent>
                <Quantity>1</Quantity>
                <Units>MB</Units>
                <Per>P1Y</Per>
            </DataExtent>
            <Acknowledgement>Please acknowledge the magnetic field and plasma Principal Investigators, Drs. E. Smith and P. Gazis, and NSSDC or GSFC-SPDF</Acknowledgement>
        </AccessInformation>
        <InstrumentID>spase://SMWG/Instrument/Pioneer10/MAG</InstrumentID>
        <InstrumentID>spase://SMWG/Instrument/Pioneer10/PLS</InstrumentID>
        <MeasurementType>MagneticField</MeasurementType>
        <MeasurementType>ThermalPlasma</MeasurementType>
        <TemporalDescription>
            <TimeSpan>
                <StartDate>1972-03-06T00:00:00</StartDate>
                <StopDate>1995-09-07T00:00:00</StopDate>
                <Note>Plasma data do not start until day 108 of 1972.  Magnetic field data end on day 321 of 1975.  Plasma data become especially sparse after ~day 90 of 1994.</Note>
            </TimeSpan>
            <Cadence>PT1H</Cadence>
        </TemporalDescription>
        <ObservedRegion>Heliosphere.Outer</ObservedRegion>
        <Parameter>
            <Name>Year</Name>
            <ParameterKey>Column1</ParameterKey>
            <Description>Four digit Year</Description>
            <Cadence>PT1H</Cadence>
            <FillValue>999.9999</FillValue>
            <Support>
                <SupportQuantity>Temporal</SupportQuantity>
            </Support>
        </Parameter>
        <Parameter>
            <Name>Day</Name>
            <ParameterKey>Column2</ParameterKey>
            <Description>Decimal Day of Year</Description>
            <Cadence>PT1H</Cadence>
            <FillValue>999.9999</FillValue>
            <Support>
                <SupportQuantity>Temporal</SupportQuantity>
            </Support>
        </Parameter>
        <Parameter>
            <Name>Hour</Name>
            <ParameterKey>Column3</ParameterKey>
            <Description>Hour</Description>
            <Cadence>PT1H</Cadence>
            <FillValue>999.9999</FillValue>
            <Support>
                <SupportQuantity>Temporal</SupportQuantity>
            </Support>
        </Parameter>
        <Parameter>
            <Name>P10 radial distance</Name>
            <ParameterKey>Column04</ParameterKey>
            <Description>Heliocentric radial distance of Pioneer 10 in AU</Description>
            <Cadence>PT1H</Cadence>
            <Units>AU</Units>
            <Support>
                <SupportQuantity>Positional</SupportQuantity>
            </Support>
        </Parameter>
        <Parameter>
            <Name>P10 latitude</Name>
            <ParameterKey>Column05</ParameterKey>
            <Description>Heliographic Inertial elevation angle of Pioneer 10 in deg</Description>
            <Cadence>PT1H</Cadence>
            <Units>deg</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Spherical</CoordinateRepresentation>
                <CoordinateSystemName>HGI</CoordinateSystemName>
            </CoordinateSystem>
            <Support>
                <SupportQuantity>Positional</SupportQuantity>
            </Support>
        </Parameter>
        <Parameter>
            <Name>P10 longitude</Name>
            <ParameterKey>Column06</ParameterKey>
            <Description>Heliographic Inertial azimuthal angle of Pioneer 10 in deg</Description>
            <Cadence>PT1H</Cadence>
            <Units>deg</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Spherical</CoordinateRepresentation>
                <CoordinateSystemName>HGI</CoordinateSystemName>
            </CoordinateSystem>
            <Support>
                <SupportQuantity>Positional</SupportQuantity>
            </Support>
        </Parameter>
        <Parameter>
            <Name>Magnetic Field Vector</Name>
            <Description>Hourly averages of RTN components of fine time scale magnetic field vectors</Description>
            <Cadence>PT1H</Cadence>
            <Units>nT</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>RTN</CoordinateSystemName>
            </CoordinateSystem>
            <Structure>
                <Size>3</Size>
                <Description>R,T,N components</Description>
                <Element>
                    <Name>R component</Name>
                    <Component>I</Component>
                    <Index>1</Index>
                    <ParameterKey>Column07</ParameterKey>
                </Element>
                <Element>
                    <Name>T component</Name>
                    <Component>J</Component>
                    <Index>2</Index>
                    <ParameterKey>Column08</ParameterKey>
                </Element>
                <Element>
                    <Name>N component</Name>
                    <Component>K</Component>
                    <Index>3</Index>
                    <ParameterKey>Column09</ParameterKey>
                </Element>
            </Structure>
            <FillValue>999.9999</FillValue>
            <Field>
                <Qualifier>Vector</Qualifier>
                <FieldQuantity>Magnetic</FieldQuantity>
            </Field>
        </Parameter>
        <Parameter>
            <Name>Magnetic Field Magnitude</Name>
            <ParameterKey>Column10</ParameterKey>
            <Description>Hourly average of fine time scale magnetic field magnitudes</Description>
            <Cadence>PT1H</Cadence>
            <Units>nT</Units>
            <FillValue>999.9999</FillValue>
            <Field>
                <Qualifier>Magnitude</Qualifier>
                <FieldQuantity>Magnetic</FieldQuantity>
            </Field>
        </Parameter>
        <Parameter>
            <Name>Flow speed</Name>
            <ParameterKey>Column11</ParameterKey>
            <Description>Hourly averaged solar wind plasma flow speed</Description>
            <Cadence>PT1H</Cadence>
            <Units>km/s</Units>
            <FillValue>9999.9</FillValue>
            <Particle>
                <ParticleType>Proton</ParticleType>
                <Qualifier>Average</Qualifier>
                <ParticleQuantity>FlowSpeed</ParticleQuantity>
            </Particle>
        </Parameter>
        <Parameter>
            <Name>Flow elevation angle</Name>
            <ParameterKey>Column12</ParameterKey>
            <Description>Hourly averaged solar wind plasma flow elevation angle</Description>
            <Cadence>PT1H</Cadence>
            <Units>deg</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Spherical</CoordinateRepresentation>
                <CoordinateSystemName>RTN</CoordinateSystemName>
            </CoordinateSystem>
            <FillValue>9999.9</FillValue>
            <Particle>
                <ParticleType>Proton</ParticleType>
                <Qualifier>DirectionAngle.ElevationAngle</Qualifier>
                <ParticleQuantity>FlowVelocity</ParticleQuantity>
            </Particle>
        </Parameter>
        <Parameter>
            <Name>Flow azimuth angle</Name>
            <ParameterKey>Column13</ParameterKey>
            <Description>Hourly averaged solar wind plasma flow azimuthal angle</Description>
            <Cadence>PT1H</Cadence>
            <Units></Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Spherical</CoordinateRepresentation>
                <CoordinateSystemName>RTN</CoordinateSystemName>
            </CoordinateSystem>
            <FillValue>9999.9</FillValue>
            <Particle>
                <ParticleType>Proton</ParticleType>
                <Qualifier>DirectionAngle.AzimuthAngle</Qualifier>
                <ParticleQuantity>FlowVelocity</ParticleQuantity>
            </Particle>
        </Parameter>
        <Parameter>
            <Name>Density</Name>
            <ParameterKey>Column14</ParameterKey>
            <Description>Hourly averaged solar wind plasma density</Description>
            <Cadence>PT1H</Cadence>
            <Units>n/cc</Units>
            <FillValue>999.9999</FillValue>
            <Particle>
                <ParticleType>Proton</ParticleType>
                <Qualifier>Average</Qualifier>
                <ParticleQuantity>NumberDensity</ParticleQuantity>
            </Particle>
        </Parameter>
        <Parameter>
            <Name>Temperature</Name>
            <ParameterKey>Column15</ParameterKey>
            <Description>Hourly averaged solar wind plasma temperature</Description>
            <Cadence>PT1H</Cadence>
            <Units>degK</Units>
            <FillValue>9999999.</FillValue>
            <Particle>
                <ParticleType>Proton</ParticleType>
                <Qualifier>Average</Qualifier>
                <ParticleQuantity>Temperature</ParticleQuantity>
            </Particle>
        </Parameter>
    </NumericalData>
</Spase>