    <Parameter>
      <Name>Spacecraft ID</Name>
      <ParameterKey>Column_1</ParameterKey>
      <Description>Spacecraft ID. Value FLT1 means Voyager 1.</Description>
      <Support>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Coordinate system</Name>
      <ParameterKey>Column_2</ParameterKey>
      <Description>Flag for coordinate system.  Virtually all values are HG,
meaning Heliographic Inertial</Description>
      <Support>
        <SupportQuantity>Other</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Year</Name>
      <ParameterKey>Column_3</ParameterKey>
      <Description>Year of the measurement, 2 digits</Description>
      <Support>
        <SupportQuantity>Temporal</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>Day of year</Name>
      <ParameterKey>Column_4</ParameterKey>
      <Description>Fractional day of year of the measurement (Jan 1 = 1).</Description>
      <ValidMin>1</ValidMin>
      <ValidMax>366.9999</ValidMax>
      <Support>
        <SupportQuantity>Temporal</SupportQuantity>
      </Support>
    </Parameter>
    <Parameter>
      <Name>B</Name>
      <ParameterKey>Column_5</ParameterKey>
      <Description>The magnetic field strength, the average of higher resolution field strengths, in nT</Description>
      <Units>nT</Units>
      <Field>
        <Qualifier>Magnitude</Qualifier>
        <Qualifier>Average</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name></Name>
      <ParameterKey>Column_6</ParameterKey>
      <Description>Magnitude of vector constituted by average field components</Description>
      <Units>nT</Units>
      <Field>
        <Qualifier>Magnitude</Qualifier>
        <Qualifier>Average</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
      <Name>Field elevation angle</Name>
      <ParameterKey>Column_7</ParameterKey>
      <Description>Elevation angle of magnetic field vector</Description>
      <Units>Deg</Units>
      <CoordinateSystem>
        <CoordinateRepresentation>Spherical</CoordinateRepresentation>
        <CoordinateSystemName>RTN</CoordinateSystemName>
      </CoordinateSystem>
      <ValidMin>-90.</ValidMin>
      <ValidMax>90.</ValidMax>
      <Field>
        <Qualifier>DirectionAngle.ElevationAngle</Qualifier>
        <Qualifier>Average</Qualifier>
        <FieldQuantity>Magnetic</FieldQuantity>
      </Field>
    </Parameter>
    <Parameter>
